<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.mycompany.proyectoed_segundoparcial.MainMenuController">
   <children>
      <Label text="Menú Principal" />
      <Button fx:id="wordSearchButton" onAction="#switchToWordSearch" text="Búsqueda de Palabras" />
      <Button fx:id="prefixSearchButton" onAction="#switchToPrefixSearch" text="Búsqueda por Prefijos" />
      <Button fx:id="dictCreateButton" onAction="#switchToDictView" text="Carga/Creacion de Dicc." />
   </children>
   <padding>
      <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
   </padding>
</VBox>
